@model ASP_MVC.Models.MatrixModel

@{
    ViewData["Title"] = "Matrix Result";
}

<h2>Result</h2>

<table class="table" style="width:300px">
    @for (int i = 0; i < Model.Size; i++)
    {
        <tr>
            @for (int j = 0; j < Model.Size; j++)
            {
                <td>@Model.Result[i, j]</td>
            }
        </tr>
    }
</table>

<a asp-action="Index">Back</a>
